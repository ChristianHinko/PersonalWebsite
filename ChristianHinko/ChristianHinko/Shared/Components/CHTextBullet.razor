@GetMarkupString()

@code
{
    [Parameter]
    public string BulletString { get; set; } = "•";

    /// <summary>
    /// String after the bullet. Useful for spacing after the bullet.
    /// </summary>
    [Parameter]
    public string PostBulletString { get; set; } = " ";

    protected MarkupString GetMarkupString()
    {
        return new MarkupString(BulletString + PostBulletString);
    }
}
